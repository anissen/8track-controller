{
  "name": "__MSG_appName__",
  "version": "1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "http://8tracks.com/*"
  ],
  "content_scripts": [
    {
      "js": [
        "scripts/main.js"
      ],
      "matches": [
        "http://8tracks.com/*"
      ]
    }
  ]
}
